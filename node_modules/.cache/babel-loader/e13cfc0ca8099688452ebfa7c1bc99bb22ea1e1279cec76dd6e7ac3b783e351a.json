\n            <div\r\n              class=\"det_image two wow fadeIn\"\r\n              data-wow-duration=\"1s\"\r\n              data-wow-delay=\"0.2s\"\r\n              data-img-url=\"/img/testimonials/ninedragonlabs-michael.jpg\"\r\n            ></div>\r\n            <div\r\n              class=\"det_image three wow fadeIn\"\r\n              data-wow-duration=\"1s\"\r\n              data-wow-delay=\"0.4s\"\r\n              data-img-url=\"/img/testimonials/turai.png\"\r\n            ></div>\r\n            <span class=\"circle yellow animPulse\"></span>\r\n            <span class=\"circle purple animPulse\"></span>\r\n            <span class=\"circle border animPulse\"></span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"brush_1 wow fadeInRight\" data-wow-duration=\"1s\">\r\n        <img src=\"/img/brushes/testimonials/1.png\" alt=\"\" />\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { testimonialIcon } from \"@/svg\";\r\nimport { Pagination } from \"swiper\";\r\nimport { Swiper, SwiperSlide } from \"vue-awesome-swiper\";\r\nexport default {\r\n  name: \"TestimonialsComponent\",\r\n  components: {\r\n    Swiper,\r\n    SwiperSlide,\r\n  },\r\n  data() {\r\n    return {\r\n      testimonialIcon,\r\n    };\r\n  },\r\n  setup() {\r\n    return {\r\n      modules: [Pagination],\r\n    };\r\n  },\r\n};\r\n</script>\r\n"],"mappings":"AAsJA,SAASA,eAAc,QAAS,OAAO;AACvC,SAASC,UAAS,QAAS,QAAQ;AACnC,SAASC,MAAM,EAAEC,WAAU,QAAS,oBAAoB;AACxD,eAAe;EACbC,IAAI,EAAE,uBAAuB;EAC7BC,UAAU,EAAE;IACVH,MAAM;IACNC;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLN;IACF,CAAC;EACH,CAAC;EACDO,KAAKA,CAAA,EAAG;IACN,OAAO;MACLC,OAAO,EAAE,CAACP,UAAU;IACtB,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}